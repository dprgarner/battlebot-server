{
  "name": "battlebot-server",
  "version": "0.1.0",
  "engines": {
    "node": "7.10.0"
  },
  "description": "A server for AI battles!",
  "main": "index.js",
  "scripts": {
    "start": "babel src --out-dir build --ignore test.js --copy-files --source-maps inline && node build/index.js",
    "test": "jest",
    "watchtest": "jest --watch",
    "coverage": "opn coverage/lcov-report/index.html",
    "codecov": "codecov"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": ["<rootDir>/build/", "<rootDir>/node_modules/"]
  },
  "author": "David Garner",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "clone": "^2.1.1",
    "dataloader": "^1.3.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "github-markdown-css": "^2.8.0",
    "graphql": "^0.10.3",
    "graphql-tools": "^1.1.0",
    "hash.js": "^1.0.3",
    "json-stable-stringify": "^1.0.1",
    "marked": "^0.3.6",
    "mongodb": "^2.2.28",
    "require-all": "^2.2.0",
    "rxjs": "^5.4.0",
    "source-map-support": "^0.4.15",
    "underscore": "^1.8.3",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "codeclimate-test-reporter": "^0.5.0",
    "codecov": "^2.2.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "opn-cli": "^3.1.0"
  }
}

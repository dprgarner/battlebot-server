{
  "name": "battlebot-server",
  "version": "1.0.0",
  "engines": {
    "node": "7.10.1"
  },
  "description": "A server for AI battles!",
  "main": "index.js",
  "scripts": {
    "start": "babel src --out-dir build --ignore test.js --copy-files --source-maps inline && node build/index.js",
    "test": "jest",
    "watchtest": "jest --watch",
    "coverage": "opn coverage/lcov-report/index.html",
    "codecov": "codecov",
    "e2e": "mocha --compilers js:babel-core/register --require babel-polyfill e2e/*spec.js"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "author": "David Garner",
  "license": "ISC",
  "dependencies": {
    "@cycle/collection": "^0.6.0",
    "@cycle/run": "^3.1.0",
    "@cycle/rxjs-run": "^7.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "clone": "^2.1.1",
    "dataloader": "^1.3.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "github-markdown-css": "^2.8.0",
    "graphql": "^0.10.3",
    "graphql-tools": "^1.1.0",
    "hash.js": "^1.0.3",
    "json-stable-stringify": "^1.0.1",
    "marked": "^0.3.6",
    "merge": "^1.2.0",
    "mongodb": "^2.2.28",
    "require-all": "^2.2.0",
    "rxjs": "^5.4.0",
    "source-map-support": "^0.4.15",
    "underscore": "^1.8.3",
    "ws": "^3.0.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.1",
    "codeclimate-test-reporter": "^0.5.0",
    "codecov": "^2.2.0",
    "jest": "^20.0.4",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "request-promise": "^4.2.1"
  }
}
